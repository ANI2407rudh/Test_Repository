<project name = "ORAAPPS" default = "compileSQL" basedir="src/main/resources">
    
	<property file="build.properties"/>
   	
    <target name = "compileSQL">
	
	<path id="class.path">
		<pathelement location=".//Lib//ojdbc6.jar"/>
	</path>
	
	<echo> message="${JDBC_DRIVER}" </echo>
	
	<sql 
		classpathref="class.path" 
		driver="${JDBC_DRIVER}"
        url="${JDBC_URL}" 
		userid= "${USER}"
        password= "${PASSWORD}"
		src="Hello_World.sql"
		print="yes"
        output="outputfile.txt"
		expandProperties="true"
		delimiter="/"
        delimitertype="row"
        >
        
		
	</sql>
	    
			
    </target>

</project>